{
  "name": "maytrok/dbapi",
  "description": "A Simple Database model with Rest Api",
  "homepage": "https://github.com/maytrok/dbapi",
  "version": "0.3.0",
  "type": "library",
  "keywords": [
    "database",
    "api",
    "restapi",
    "database model",
    "simple"
  ],
  "authors": [
    {
      "name": "Maytrok",
      "email": "bluna.el@gmx.de",
      "role": "Developer"
    }
  ],
  "license": "BSD-3-Clause",
  "autoload": {
    "files": [
      "./bin/bootstrap.php"
    ],
    "exclude-from-classmap": [
      "test/",
      "examples/"
    ],
    "psr-4": {
      "dbapi\\": "src"
    }
  },
  "require": {
    "php": ">=5.4.0",
    "monolog/monolog": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "phan/phan": "^2.4",
    "firebase/php-jwt": "^5.0"
  },
  "suggest": {
    "firebase/php-jwt": "JWT Auth: Required if using JWTAuththenticate Model"
  }
}
